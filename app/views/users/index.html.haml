- model_class = User
- content_for :actions do
  %li
    = link_to 'new user', [:new, :user]
.text-center.filter-label
.letters.left
  - ('A'..'M').each do |letter|
    .filter-letter= letter
.letters.right
  - ('N'..'Z').each do |letter|
    .filter-letter= letter
.row
  .col-xs-6.col-sm-3.col-md-2.col-sm-2.col-lg-2
  = link_to ("/anonymous"), :title => "Cash Payment" do
    .panel.panel-default.text-center
      .panel-body
        %h6= "Cash Payment"
.row{id: "user_preview"}
  = render 'user_view', users: @users
